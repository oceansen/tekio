@ARTICLE{Arboledaa,
  author = {Arboleda, Hugo and Kastler, Alfred and Cedex, F- Nantes},
  title = {{Dealing with Fine-Grained Configurations in Model-Driven SPLs}},
  journal = {Transformation},
  year = {2009},
  pages = {1--10},
  abstract = {In Model-Driven SPL approaches, the derivation of a product starts
	from a domain application model. This model is transformed using
	model transformations, which are se- lected according to variants
	included in configurations, un- til a product is obtained. There
	are at least two drawbacks to these approaches. First, the selection
	of variants affects the whole domain application model, impeding
	fine-grained configurations, i.e. configurations at the level of
	each ele- ment in the model, and second, model transformations are
	coupled with variants which make their maintenance and adaptation
	difficult. We present an approach that uses meta- modeling and feature
	modeling. A novelty in our approach is the possibility of configuring
	a product from a domain application model in which elements can be
	bound sepa- rately to features. These fine-grained configurations
	are an input to the derivation process which uses decision models
	and Aspect-Oriented Programming, facilitating the reuse, adaptation
	and composition of model transformations.},
  annote = {Our approach facilitates the binding of model elements to particular
	features, prevents the creation of in- valid bindings and makes possible
	the dynamic adaptation of model transformations without coupling
	them with infor- mation about variability choices. We achieve this
	by using fine-grained configurations, constraint models, and decision
	models which take the set of bindings defining a configura- tion
	to generate different model transformation scheduling. We use Aspect-Oriented
	Programming (AOP) [13, 17] to build the scheduling of the model transformations,
	i.e. the order in which model transformations process model ele-
	ments to accomplish a desired derivation},
  file = {:Users/shurtado/Documents/pds/2009/Arboleda, Kastler, Cedex/Arboleda, Kastler, Cedex - 2009 - Dealing with Fine-Grained Configurations in Model-Driven SPLs.pdf:pdf}
}

