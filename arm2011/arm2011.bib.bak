@ARTICLE{Botterweck2009,
  author = {Botterweck, Goetz and Pleuss, Andreas and Polzer, Andreas and Kowalewski,
	Stefan},
  title = {{Towards feature-driven planning of product-line evolution}},
  journal = {Proceedings of the First International Workshop on Feature-Oriented
	Software Development - FOSD '09},
  year = {2009},
  pages = {109},
  abstract = {Industries that successfully apply product line approaches often operate
	in markets that are well established and have a strategic perspective.
	Consequently, such organizations have a tendency towards long-term
	planning of products and product lines. Although there are numerous
	approaches for efficient product line engineering, there is surprisingly
	little support for a long-term, strategic perspective and an evolu-
	tion of product lines. To address these challenges, we aim to integrate
	evolution into model-driven product line engi- neering. In particular,
	we explore how feature models can be applied to describe the evolution
	of product lines. The paper contributes (i) concepts for describing
	the evolution of product lines with feature models, (ii) a corresponding
	framework, which puts this into a bigger context and (iii) three
	scenarios that show how this framework can be ap- plied. The concepts
	are motivated with examples from au- tomotive software engineering
	and embedded systems, which are industries with a strong affinity
	to product lines, where long term planning of the product portfolio
	are common strategies.},
  address = {New York, New York, USA},
  annote = {We intend to use feature models to handle the evolution of product
	lines. In this paper, we take first steps towards this goal by analyzing
	the research problem and defining our ap- proach on a conceptual
	level: We motivate our research by examples from automotive embedded
	systems (Section 2). The paper contributes (i) concepts for describing
	the evo- lution of product lines with feature models (Section 3),
	(ii) a corresponding framework which puts this into a bigger context
	(Section 4) and (iii) three scenarios that apply this framework (Section
	5). We conclude with a brief overview of related work (Section 6).},
  doi = {10.1145/1629716.1629737},
  file = {:Users/shurtado/Documents/pds/2009/Botterweck et al./Botterweck et al. - 2009 - Towards feature-driven planning of product-line evolution.pdf:pdf},
  isbn = {9781605585673},
  keywords = {all or part of,evolution,feature modeling,is granted without fee,or
	hard copies of,permission to make digital,personal or classroom use,product
	line engineering,provided that copies are,this work for},
  publisher = {ACM Press},
  url = {http://portal.acm.org/citation.cfm?doid=1629716.1629737}
}

